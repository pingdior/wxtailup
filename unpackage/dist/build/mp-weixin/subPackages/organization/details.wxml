<view class="container"><view class="page-header"><view class="page-section page-section-spacing swiper"><swiper style="height:345rpx;" indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}"><block wx:for="{{orgHomeData.imageIds}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><swiper-item class="swiper-item"><image class="slide-image" bindtap="tapBanner" data-id="{{item}}" src="{{item}}" mode="{{center}}"></image></swiper-item></block></block></swiper></view></view><view class="page-body"><image class="avatar" src="{{orgHomeData.headImgUrl}}" mode="center"></image><view class="w-block page-body-info-block"><view class="page-body-navbar" hover-class="none" hover-stop-propagation="false"><block wx:if="{{isSelf}}"><button class="button-middle" size="mini" type="default" data-event-opts="{{[['tap',[['toEdit',['$event']]]]]}}" bindtap="__e"><text>编辑</text></button></block><block wx:else><button class="button-middle" size="mini" type="{{orgHomeData.focused?'default':'primary'}}" data-event-opts="{{[['tap',[['toFocus',['$event']]]]]}}" bindtap="__e"><block wx:if="{{!orgHomeData.focused}}"><text class="iconfont icon-add"></text></block><text>{{(orgHomeData.focused?"已":"")+"关注"}}</text></button></block><button class="button-right color-text3" size="mini" type="default" data-event-opts="{{[['tap',[['toShareBase',['$event']]]]]}}" bindtap="__e"><text class="iconfont icon-share">分享</text></button></view><view class="page-body-info"><view class="page-body-user"><text class="user-name">{{orgHomeData.name}}</text><view class="user-help"><icon class="w-icon w-icon-medium icon-cat"></icon><text style="margin-right:16rpx;"><text style="margin-left:2rpx;">{{orgHomeData.dogNum}}</text>汪汪</text><icon class="w-icon w-icon-medium icon-dog"></icon><text><text style="margin-left:2rpx;">{{orgHomeData.catNum}}</text>喵喵</text></view></view><view class="page-body-fence default-box bg-gray2" hover-class="none" hover-stop-propagation="false"><view class="fence-container"><text class="font-bold">{{orgHomeData.canNum}}</text><text class="font12 color-text2">稀罕</text></view><view class="fence-container"><text class="font-bold">{{orgHomeData.followNum}}</text><text class="font12 color-text2">关注</text></view><view class="fence-container"><text class="font-bold">{{orgHomeData.fanNum}}</text><text class="font12 color-text2">粉丝</text></view></view></view></view><van-tabs vue-id="0a337150-1" active="{{active}}" data-event-opts="{{[['^change',[['onChange']]]]}}" data-com-type="wx" bind:change="__e" bind:__l="__l" vue-slots="{{['default']}}"><van-tab vue-id="{{('0a337150-2')+','+('0a337150-1')}}" title="毛孩子" data-com-type="wx" bind:__l="__l" vue-slots="{{['default']}}"><view class="tab-content"><block wx:if="{{isSelf}}"><view data-event-opts="{{[['tap',[['toAddAnimal',['$event']]]]]}}" class="w-block list-wrapper" bindtap="__e"><view class="is-add-container add-box"><view class="is-add"><view class="add-line"></view><view class="add-line rotate"></view></view><text class="add-text">新增</text></view></view></block><block wx:for="{{reverseAnimalBriefs}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="w-block list-wrapper"><view class="list-image-wrapper"><image class="list-image" src="{{item.headImageId}}" data-event-opts="{{[['tap',[['toAnimalDetails',['$0'],[[['reverseAnimalBriefs','',index]]]]]]]}}" bindtap="__e"></image></view><view class="list-info-wrapper" hover-class="none" hover-stop-propagation="false"><view class="list-info-top"><text class="list-info-tilte">{{item.name}}</text><text class="{{[(true)?'list-info-tag':'']}}">{{item.succorStatus}}</text></view><view class="list-info-age"><text style="margin-right:3rpx;">{{item.age}}</text><block wx:if="{{item.succorTime}}"><text>{{item.succorTime+"救助"}}</text></block></view><view class="list-info-cake color-warning"><image class="user-cake" src="/static/images/cake/cake.png"></image><text class="cakes">收到稀罕</text>·<text>{{item.canNum}}</text></view></view><view><button data-event-opts="{{[['tap',[['toAnimalDetails',['$0'],[[['reverseAnimalBriefs','',index]]]]]]]}}" class="list-info-button" bindtap="__e">成长记录</button><block wx:if="{{isSelf}}"><button data-event-opts="{{[['tap',[['toDelete',['$0',index],[[['reverseAnimalBriefs','',index]]]]]]]}}" class="list-info-button" bindtap="__e">删除</button></block></view></view></block></block><view class="loading-more"><text class="loading-more-text">没有更多数据了</text></view></view></van-tab><van-tab vue-id="{{('0a337150-3')+','+('0a337150-1')}}" title="领养与答疑" data-com-type="wx" bind:__l="__l" vue-slots="{{['default']}}"><view class="tab-content"><view class="faq-body"><block wx:if="{{isSelf}}"><view data-event-opts="{{[['tap',[['toAddFaq',['$event']]]]]}}" class="w-block list-wrapper" bindtap="__e"><view class="is-add-container add-box"><view class="is-add"><view class="add-line"></view><view class="add-line rotate"></view></view><text class="add-text">新增</text></view></view></block><view class="w-block">{{''+(baseInfo.intro||"无")+''}}</view><block wx:for="{{orgHomeData.baseQas}}" wx:for-item="item" wx:for-index="index" wx:key="id"><uni-card vue-id="{{('0a337150-4-'+index)+','+('0a337150-3')}}" title="{{item.ask}}" note="Tips" bind:__l="__l" vue-slots="{{['default']}}"><text class="content-box-text">{{item.answer}}</text><block wx:if="{{isSelf}}"><block slot="footer"><view class="footer-box"><view data-event-opts="{{[['tap',[['onEdit',['$0',index],[[['orgHomeData.baseQas','id',item.id]]]]]]]}}" class="footer-box__item" bindtap="__e">编辑</view><view data-event-opts="{{[['tap',[['onDelete',['$0',index],[[['orgHomeData.baseQas','id',item.id]]]]]]]}}" class="footer-box__item" bindtap="__e">删除</view></view></block></block></uni-card></block></view></view></van-tab><block wx:if="{{isSelf&&animalApllies.length>0}}"><van-tab vue-id="{{('0a337150-5')+','+('0a337150-1')}}" title="领养申请" data-com-type="wx" bind:__l="__l" vue-slots="{{['default']}}"><view class="tab-content"><block wx:for="{{animalApllies}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="w-block list-wrapper"><view class="list-image-wrapper"><image class="list-image" src="{{animalAplliesBaseInfos[index].headImageId}}"></image></view><view class="list-info-wrapper"><view class="list-info-top"><text class="list-info-tilte">{{animalAplliesBaseInfos[index].name}}</text><text class="list-info-tag" style="color:#64c8b3;">{{item.isAdopted==0?"待领养":"已领养"}}</text></view><view class="list-info-middle"><text>{{"领养人 "+item.nickName}}</text><button data-event-opts="{{[['tap',[['toAnimalAdoptDetails',['$0'],[[['animalApllies','',index]]]]]]]}}" class="list-info-button" style="margin-right:0rpx;" bindtap="__e">申请详情</button></view><view class="list-info-botton"><text>{{item.updateTime}}</text></view></view></view></block></block><block wx:if="{{noData}}"><view class="loading-more"><text class="loading-more-text">没有更多数据了</text></view></block></view></van-tab></block></van-tabs></view></view>